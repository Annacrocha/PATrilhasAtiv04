using System;

namespace atividade
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Você possui um diploma de ensino superior?");
			bool diploma = Console.ReadLine().ToLower() == "sim";
			Console.WriteLine("Insira sua idade:");
			int idade = Convert.ToInt32(Console.ReadLine());
			
				if (idade >= 21 && diploma) {
				Console.WriteLine("Parabéns, você está qualificado para a vaga !");
			} else {
				Console.WriteLine("Infelizmente você não foi qualificado");
			}
			                  
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
