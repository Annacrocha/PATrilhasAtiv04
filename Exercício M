using System;

namespace ATIVIDADE_4_
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Você não é um membro inativo do clube?");
			string resposta = Console.ReadLine().ToLower();
			
			Console.WriteLine(" ");
			
			if (resposta == "não") {
				Console.WriteLine("Por favor, atualize sua inscrição para continuar usufruindo dos benefício do clube");
			} else {
				Console.WriteLine("Volte a inatividade !");
			}
			
			
			Console.WriteLine(" ");
			                  
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
